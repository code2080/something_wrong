  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom --passWithNoTests",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rm -rf dist && rollup -c",
    "start": "rollup -c -w --environment BUILD:development",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:diff": "f(){ eslint $(git diff --name-only --diff-filter=ACMRTUXB origin/${1:-development} | grep  -E \"([^/])(src).*(.[jt]sx?)$)\");};f",
    "modified": "f(){  echo $(git diff --name-only --diff-filter=ACMRTUXB origin/${1:-development} | grep  -E \"([^/])(src).*(.[jt]sx?)$)\");};f",
    "lint:diff:dev": "yarn lint:diff development",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier ./src --check",
    "prettier:diff": "f(){ prettier $(git diff --name-only --diff-filter=ACMRTUXB origin/${1:-development} | grep  -E \"(.js$|.ts$|.tsx$)\") --check;};f",
    "prettier:fix:diff": "f(){ prettier $(git diff --name-only --diff-filter=ACMRTUXB origin/${1:-development} | grep  -E \"(.js$|.ts$|.tsx$)\") --write;};f",
    "prettier:fix": "prettier ./src --write",
    "tscompile": "tsc",
    "tscompile:watch": "tsc -w",
    "ts:lint": "tsc --noemit -p tsconfig.json",
    "prepare": "husky install && yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "preversion": "yarn lint && yarn prettier && yarn test",
    "postversion": "yarn publish . --new-version $npm_package_version && git push && git push --tags && echo \"Successfully released version $npm_package_version!\"",
    "sb": "yarn storybook",
    "storybook": "start-storybook -p 6006",
    "storybook:export": "build-storybook"
  },
